      ASPNETCORE_ENVIRONMENT: Production
      ConnectionStrings__DefaultConnection: server=mysql-container;port=3306;database=prodb;user=root;password=Password@12345
    image: samplewebapi
    networks:
      default: null
    ports:
      - mode: ingress
        target: 5000
        published: "5000"
        protocol: tcp
